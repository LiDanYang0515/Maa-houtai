<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.swjtuhc.demo.mapper.ScheduleMapper">
	<insert id="insertsc" parameterType="edu.swjtuhc.demo.model.Schedule">
		insert into schedule
		(creat_Date,textString)
		values
		(#{creat_Date},#{textString})
	</insert>
	<select id="selectScheduleBycreate_Date" parameterType="java.sql.Date" resultType="edu.swjtuhc.demo.model.Schedule">
		select * from schedule
		where
		create_Date = #{create_Date}
	</select>
</mapper>